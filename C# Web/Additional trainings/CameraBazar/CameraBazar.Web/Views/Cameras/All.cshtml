@model IEnumerable<CameraListingModel>

@{
    ViewData["Title"] = "All cameras";
}

<div class="container col-md-12">
    <div class="container col-md-10 col-md-offset-1">
        <div class="row">
            <div class="col-md-9">
                <h2>@ViewData["Title"]</h2>
            </div>
            <div class="col-md-3 text-right">
                <a class="btn btn-lg btn-info btn-block" href="/cameras/add">Add</a>
            </div>
        </div>
        <hr />

        @await Html.PartialAsync("_CameraListPartial", Model)
    </div>
</div>
